<?xml version="1.0" encoding="UTF-8" ?>
  
<widget xmlns   = "http://www.w3.org/ns/widgets"
        xmlns:gap   = "http://phonegap.com/ns/1.0"
        id          = "com.FreeWay"
        versionCode = "10" 
        version     = "1.0.0" >

    <!-- versionCode is optional and Android only -->

    <name>FreeWay</name>
     <icon src="images/colorsIcon.png" width="128" height="128"/>
    <description>
       The FreeWay App 
    </description>
    
    <!-- Define app splash screen for each platform. -->
    <gap:splash src="images/splash.png"      gap:platform="android" gap:density="ldpi" />
    <gap:splash src="images/splash.png"      gap:platform="android" gap:density="mdpi" />
    <gap:splash src="images/splash.png"      gap:platform="android" gap:density="hdpi" />
    <gap:splash src="images/splash.png"      gap:platform="android" gap:density="xhdpi" />
    <gap:splash src="images/splash.png"      gap:platform="blackberry" />
    <gap:splash src="images/splash.png"      gap:platform="ios"     width="320" height="480" />
    <gap:splash src="images/splash.png"      gap:platform="ios"     width="640" height="960" />
    <gap:splash src="images/splash.png"      gap:platform="ios"     width="640" height="1136" />
    <gap:splash src="images/splash.png"      gap:platform="ios"     width="768" height="1024" />
    <gap:splash src="images/splash.png"      ap:platform="ios"     width="1024" height="768" />
    <gap:splash src="images/splash.png"      gap:platform="winphone" />
<!--<content src="search.html"/>-->
   
</widget>